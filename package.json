{
  "name": "esimsdata",
  "version": "1.0.0",
  "description": "eSIMsData Telegram Mini App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "prefill-cache": "node scripts/prefill-cache.js",
    "refresh-local-plans": "node scripts/refresh-local-plans.js",
    "build:css": "node ./node_modules/.bin/tailwindcss -i ./admin/css/tailwind-input.css -o ./admin/css/tailwind.css --minify",
    "watch:css": "node ./node_modules/.bin/tailwindcss -i ./admin/css/tailwind-input.css -o ./admin/css/tailwind.css --watch",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "esim",
    "telegram",
    "miniapp"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.19"
  }
}
