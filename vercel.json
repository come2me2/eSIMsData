{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/flags/:path*",
      "destination": "/flags/:path*"
    }
  ],
  "headers": [
    {
      "source": "/flags/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    }
  ]
}

